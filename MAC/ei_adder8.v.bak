module ei_adder8 (input [7:0] a, b, input cin, output [7:0] sum, output cout);
    wire [8:0] c;
    assign c[0] = cin;
    genvar i;
    generate
        for(i=0; i<8; i=i+1) begin: LOOP
            full_adder fa (.a(a[i]), .b(b[i]), .cin(c[i]), .sum(sum[i]), .cout(c[i+1]));
        end
    endgenerate
    assign cout = c[8];
endmodule